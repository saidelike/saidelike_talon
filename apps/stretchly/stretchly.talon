# global shortcuts

# Postpone/finish current break
stretch (end|stop): key(shift-ctrl-f1)
# Toggling between disabling breaks and running breaks
stretch (toggle|switch): key(shift-ctrl-f2)
# Skip to one of the next breaks
stretch next [break]: key(shift-ctrl-f3)
stretch next short: key(shift-ctrl-f4)
stretch next long: key(shift-ctrl-f5)
# Reset the breaks to startup state
stretch reset: key(shift-ctrl-f6)